{
	"name": "ecomailcz/flexibee-client",
	"description": "Basic client for make flexibee requests",
	"author": "Petr Kr√°l (petr@ecomail.cz)",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"ext-simplexml": "*",
		"ext-curl": "*",
		"ext-json": "*",
		"php": "^7.2",
		"consistence/consistence": "^2.0",
		"jwage/purl": "^0.0.10",
		"rakit/validation": "^1.1"
	},
	"require-dev": {
		"slevomat/coding-standard": "^5.0",
		"phpstan/phpstan": "^0.11.0",
		"phpunit/phpunit": "^8.0",
		"phing/phing": "^2.16",
		"fzaninotto/faker": "^1.7",
		"roave/security-advisories": "dev-master",
		"sllh/composer-versions-check": "^2.0",
		"wapmorgan/php-code-fixer": "^2.0",
		"sebastian/phpcpd": "^4.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpstan/phpstan-phpunit": "^0.11.0"
	},
	"autoload": {
		"psr-4": {
			"EcomailFlexibee\\": ["src/"],
			"EcomailFlexibeeTest\\": ["tests/"]
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
