{
	"name": "ecomailcz/flexibee-client",
	"description": "ECOMAIL.CZ - Simple client for communication with FlexiBee API",
	"license": [
		"MIT"
	],
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-simplexml": "*",
		"ecomailcz/purl": "^2.0",
		"rakit/validation": "^1.4",
		"slevomat/consistence": "^2.1"
	},
	"require-dev": {
		"brianium/paratest": "^7.2",
		"ergebnis/composer-normalize": "^2.34.0",
		"fzaninotto/faker": "^v1.9.2",
		"mockery/mockery": "^1.6.4",
		"pekral/phpcs-rules": "dev-master",
		"pekral/phpstan-rules": "dev-master",
		"pekral/rector-rules": "dev-master",
		"phing/phing": "^3.0",
		"php-parallel-lint/php-parallel-lint": "^1.3.2",
		"phpstan/extension-installer": "^1.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"EcomailFlexibeeTest\\": [
				"tests/"
			],
			"EcomailFlexibee\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"phpstan/extension-installer": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"sort-packages": true
	}
}
